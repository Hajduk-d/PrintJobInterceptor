<Application x:Class="PrintJobInterceptor.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PrintJobInterceptor.Desktop"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/PresentationFramework.Fluent;component/Themes/Fluent.xaml" />
                <ResourceDictionary Source="/Views/Printer/PrinterTemplates.xaml"/>
                <ResourceDictionary Source="/Views/PrintJob/PrintJobTemplates.xaml"/>
                <ResourceDictionary Source="Views/Document/DocumentTemplates.xaml"/>
                <ResourceDictionary Source="/Styles/Styling.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
